{
    "name": "tester",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ],
            "all_frames": false
        }
    ],
    "background": {
        "service_worker": "bg.js",
        "mode": "module"
    }
}